{
  "name": "backstop-retry-failed-scenarios",
  "version": "1.3.0",
  "description": "A wrapper command to retry failed scenario for BackstopJS",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "ci": "npm run typecheck && npm run lint && npm run test"
  },
  "author": "hitode909",
  "repository": {
    "type": "git",
    "url": "https://github.com/hitode909/backstop-retry-failed-scenarios.git"
  },
  "license": "ISC",
  "keywords": ["backstopjs", "testing", "visual-regression", "retry"],
  "devDependencies": {
    "@types/command-line-args": "^5.2.3",
    "@types/command-line-usage": "^5.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "test-fixture": "^2.4.1",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "escape-string-regexp": "^4.0.0",
    "fast-xml-parser": "^4.5.0"
  },
  "bin": {
    "backstop-retry-failed-scenarios": "dist/cli.js"
  }
}
